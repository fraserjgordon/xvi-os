{
    "configurations": [
        {
            "name": "XVI GCC x86_64",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/C++/StdLib/Include"
            ],
            "compilerPath": "/opt/cross/bin/x86_64-xvi-g++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "gcc-x64",
            "compileCommands": "${workspaceRoot}/out/compile_commands.json",
            "configurationProvider": "vector-of-bool.cmake-tools",
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}