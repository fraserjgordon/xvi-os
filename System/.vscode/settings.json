{
    "cmake.buildDirectory": "${workspaceRoot}/.xvi.build",
    "cmake.configureOnOpen": false,
    "cmake.preferredGenerators": [ "Ninja", "Unix Makefiles" ],
    "C_Cpp.intelliSenseEngineFallback": "Enabled",
    "files.associations": {
        "*.S": "asm",
        "array": "cpp",
        "functional": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "memory": "cpp",
        "*.tcc": "cpp",
        "string_view": "cpp",
        "numeric": "cpp",
        "optional": "cpp",
        "bit": "cpp"
    },
    "cmake.copyCompileCommands": "${workspaceRoot}/.xvi.build/compile_commands.json",
    "restructuredtext.sphinxBuildPath": "/home/fraserjgordon/.local/bin/sphinx-build",
    "restructuredtext.linter.executablePath": "/home/fraserjgordon/.local/bin/rstcheck",
    "restructuredtext.confPath": "${workspaceFolder}/Docs"
}
